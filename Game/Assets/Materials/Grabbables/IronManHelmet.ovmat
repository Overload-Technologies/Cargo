<root>
    <shader>:Shaders\StandardPBR.glsl</shader>
    <settings>
        <blendable>false</blendable>
        <backface_culling>false</backface_culling>
        <frontface_culling>false</frontface_culling>
        <depth_test>true</depth_test>
        <depth_writing>true</depth_writing>
        <color_writing>true</color_writing>
        <gpu_instances>1</gpu_instances>
    </settings>
    <uniforms>
        <uniform>
            <name>u_Albedo</name>
            <value>
                <x>1</x>
                <y>1</y>
                <z>1</z>
                <w>1</w>
            </value>
        </uniform>
        <uniform>
            <name>u_AlbedoMap</name>
            <value>Textures\IronmanHelmet\ALBEDO.tga.png</value>
        </uniform>
        <uniform>
            <name>u_AmbientOcclusionMap</name>
            <value>Textures\IronmanHelmet\AO.tga.png</value>
        </uniform>
        <uniform>
            <name>u_EnableNormalMapping</name>
            <value>1</value>
        </uniform>
        <uniform>
            <name>u_Metallic</name>
            <value>0.89999998</value>
        </uniform>
        <uniform>
            <name>u_MetallicMap</name>
            <value>?</value>
        </uniform>
        <uniform>
            <name>u_NormalMap</name>
            <value>Textures\IronmanHelmet\NRM.tga.png</value>
        </uniform>
        <uniform>
            <name>u_Roughness</name>
            <value>0.2</value>
        </uniform>
        <uniform>
            <name>u_RoughnessMap</name>
            <value>?</value>
        </uniform>
        <uniform>
            <name>u_TextureOffset</name>
            <value>
                <x>0</x>
                <y>0</y>
            </value>
        </uniform>
        <uniform>
            <name>u_TextureTiling</name>
            <value>
                <x>1</x>
                <y>1</y>
            </value>
        </uniform>
    </uniforms>
</root>
